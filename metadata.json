{
  "name": "puppet-mrepo",
  "version": "2.0.0",
  "author": "Vox Pupuli",
  "summary": "Configures and manages mrepo mirrors",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-mrepo",
  "project_page": "https://github.com/voxpupuli/puppet-mrepo",
  "issues_url": "https://github.com/voxpupuli/puppet-mrepo/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
